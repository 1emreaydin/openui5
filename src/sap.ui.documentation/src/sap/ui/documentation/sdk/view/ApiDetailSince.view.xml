<mvc:View controllerName="sap.ui.documentation.sdk.controller.ApiDetailIndexDeprecatedExperimental"
		  xmlns="sap.uxap"
		  xmlns:m="sap.m"
		  xmlns:mvc="sap.ui.core.mvc"
		  xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
	<ObjectPageLayout enableLazyLoading="false"
					  showAnchorBarPopover="true"
					  upperCaseAnchorBar="false"
					  sections="{/}">
		<headerTitle>
			<ObjectPageHeader id="title" objectTitle="Index by Version"/>
		</headerTitle>

		<sections>
			<ObjectPageSection
					titleUppercase="false"
					title="As of {name}">
				<subSections>
					<ObjectPageSubSection>
						<blocks>
							<m:List updateFinished="._modifyLinks"
									items="{ path: 'apis', templateShareable: 'false'}"
									showSeparators="Inner">
								<m:items>
									<m:FeedListItem
											showIcon="false"
											sender="{parts : [{path : 'control'},
													{path : 'entityName'}, {path: 'type'}],
													formatter : '.formatSenderLink'}"
											text="{path : 'text', formatter: '.formatDescription'}"
											app:control="{control}"
											app:entity="{entityName}"
											app:type="{type}"
											app:static="{static}"/>
								</m:items>
							</m:List>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>
