{
	"_version": "1.15.0",
	"sap.app": {
		"id": "card.explorer.parameters.list.card",
		"type": "card",
		"title": "Sample of a Card with Parameters",
		"subTitle": "Sample of an Card with Parameters"
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://switch-classes"
		}
	},
	"sap.card": {
		"configuration": {
			"parameters": {
				"city": {
					"value": "New York"
				}
			}
		},
		"type": "List",
		"header": {
			"title": "Weather forecast for {{parameters.city}}",
			"subTitle": "{{parameters.NOW_ISO}}, locale: {{parameters.LOCALE}}"
		},
		"content": {
			"data": {
				"request": {
					"url": "//api.apixu.com/v1/forecast.json?key=4fbdcdb5a8c24190bc3145021191302&q={{parameters.city}}&days=7"
				},
				"path": "/forecast/forecastday"
			},
			"item": {
				"icon": {
					"src": "{day/condition/icon}"
				},
				"title": "For {date}",
				"description": "{day/avgtemp_c}Â°C, {day/condition/text}"
			}
		}
	}
}
