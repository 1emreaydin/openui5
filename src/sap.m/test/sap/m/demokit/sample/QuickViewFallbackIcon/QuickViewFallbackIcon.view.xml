<core:View height="100%"
		xmlns:core="sap.ui.core"
		xmlns="sap.m"
		xmlns:l="sap.ui.layout"
		controllerName="sap.m.sample.QuickViewFallbackIcon.QuickViewFallbackIcon">
	<Page showHeader="false" class="sapUiContentPadding" showNavButton="false">
		<l:VerticalLayout>
			<l:content>
				<Button id="showQuickView"
						class="sapUiSmallMarginBottom"
						text="Employee QuickView"
						width="200px"
						press="handleButtonPress" />
			</l:content>
		</l:VerticalLayout>
	</Page>
</core:View>
