<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:w="sap.ui.integration.widgets"
    controllerName="sap.f.cardsdemo.controller.Services"
	displayBlock="true">

    <Page title="{i18n>appTitle}">

        <subHeader>
            <Toolbar>
                <ToolbarSpacer/>
                <Title text="Card services"/>
                <ToolbarSpacer/>
            </Toolbar>
        </subHeader>

        <content>
            <Panel>
                <w:Card id="listCount" class="sapUiMediumMargin" manifest="./model/manifests/manifest_navigationService.json" width="400px" height="auto"/>
                <w:Card id="listCount2" class="sapUiMediumMargin" manifest="./model/manifests/manifest_navigationService2.json" width="400px" height="auto"/>
            </Panel>
        </content>
    </Page>
</mvc:View>