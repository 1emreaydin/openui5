<mvc:View
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:f="sap.f"
	xmlns:w="sap.ui.integration.widgets"
	displayBlock="true">

	<Page title="{i18n>appTitle}">

		<subHeader>
			<Toolbar>
				<ToolbarSpacer/>
				<Title text="Table Content Card "/>
				<ToolbarSpacer/>
			</Toolbar>
		</subHeader>

		<content>

			<f:CardContainer>
				<w:Card id="tablecard1" manifest="{manifests>/tableManifests/tablecard1}" height="auto" class="sapUiMediumMarginEnd">
					<w:layoutData>
						<f:CardItemLayoutData columns="7" />
					</w:layoutData>
				</w:Card>
				<w:Card id="tablecard3" manifest="{manifests>/tableManifests/tablecard3}" height="auto">
					<w:layoutData>
						<f:CardItemLayoutData columns="4" />
					</w:layoutData>
				</w:Card>
				<w:Card id="tablecard4" manifest="{manifests>/tableManifests/tablecard4}" height="auto">
					<w:layoutData>
						<f:CardItemLayoutData columns="5" />
					</w:layoutData>
				</w:Card>
			</f:CardContainer>
		</content>
	</Page>
</mvc:View>