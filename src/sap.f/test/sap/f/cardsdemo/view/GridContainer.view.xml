<mvc:View
		xmlns="sap.m"
		xmlns:mvc="sap.ui.core.mvc"
		xmlns:f="sap.f"
		xmlns:w="sap.ui.integration.widgets"
		displayBlock="true">
	<Page title="{i18n>appTitle}">
		<subHeader>
			<Toolbar>
				<ToolbarSpacer />
				<Title text="Card Layout"/>
				<ToolbarSpacer />
			</Toolbar>
		</subHeader>
		<content>
			<f:GridContainer class="sapUiSmallMargin">
				<f:layout>
					<f:GridContainerSettings rowSize="5rem" columnSize="5rem" gap="1rem" />
				</f:layout>
				<f:layoutS>
					<f:GridContainerSettings rowSize="4rem" columnSize="4rem" gap="0.5rem" />
				</f:layoutS>
				<w:Card manifest="{manifests>/listContent/card13}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="3" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/analyticalContent/line}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="4" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/listContent/card13}">
					<w:layoutData>
						<f:GridContainerItemLayoutData rows="7" columns="4" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/analyticalContent/stackedBar}">
					<w:layoutData>
						<f:GridContainerItemLayoutData rows="5" columns="4" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/analyticalContent/stackedColumn}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="5" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/analyticalContent/donut}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="6" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/listContent/card13}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="3" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/analyticalContent/line}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="4" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/analyticalContent/stackedBar}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="4" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/listContent/card13}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="5" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/analyticalContent/stackedColumn}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="3" />
					</w:layoutData>
				</w:Card>
				<w:Card manifest="{manifests>/analyticalContent/donut}">
					<w:layoutData>
						<f:GridContainerItemLayoutData columns="8" />
					</w:layoutData>
				</w:Card>
			</f:GridContainer>
		</content>
	</Page>
</mvc:View>
